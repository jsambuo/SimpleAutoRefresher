{
    "manifest_version": 2,
  
    "name": "rfrsh",
    "description": "This extension will refresh the active tab in a given time interval",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "rfrshicon_44.png",
     "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
     "activeTab"
    ],
    "web_accessible_resources": [
        "rfrshicon_44.png"
    ]
}