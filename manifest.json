{
    "manifest_version": 2,
  
    "name": "rfrsh",
    "description": "This extension will refresh the active tab in a given time interval",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
     "activeTab"
    ]
}